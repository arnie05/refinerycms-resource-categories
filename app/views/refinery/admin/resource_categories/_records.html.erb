<%= render 'refinery/admin/search_header', :url => refinery.admin_resource_categories_path %>
<div class='pagination_container'>
  <% if @resource_categories.any? %>
    <%= render 'resource_categories' %>
  <% else %>
    <p>
      <strong>
        <%= t('.no_items_yet') %>
      </strong>
    </p>
  <% end %>
</div>
